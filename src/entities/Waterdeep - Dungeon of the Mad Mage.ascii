items {
  common {
    name: "Circlet of Human Perfection"
    worlds: "Generic"
    description: "Only humanoids can attune to it. The circlet transforms its attuned wearer into an attractive human of average height and weight. The circlet chooses the physical characterists of teh new form, such as age, gender, skin color, hair color, and voice. Except for size, the wearer's statistics and racial traits don't change, nor do items worn or carried by the wearer. Removing the circlet ends the effect."
    short_description: "The circlet transforms its attuned wearer into an attractive human of average height and weight."
    references {
      name: "Waterdeep: Dungeon of the Mad Mage"
      id: "WTC C4659"
      pages {
        low: 30
        high: 30
      }
    }
    images: "circlet of human perfection.jpg"
  }
  type: CIRCLET
  weight {
    pounds {
      nominator: 1
      denominator: 4
    }
  }
  rarity: UNCOMMON
  attunement: true
  size: SMALL
  substance {
    material: IRON
    thickness {
      inches {
        nominator: 1
        denominator: 4 
      }
    }
  }
}

items {
  common {
    name: "Halaster's Coin"
    worlds: "Generic"
    description: "A gold-painted stone coin. Engraved on each side of the loose coin is Halaster's rune. A \\Spell{detect magic} spell reveals an aura of abjuration magic around the coin, and an \\Spell{identify} spell or similar magic reveals its magical property.\n\nWhen an elder rune triggers within 60 feet of the creature that possesses the coin, the creature can use its reaction and the coin to change the elder rune's bane effect into a boon effect, or vice versa. After the coin is used once, it disappears in a flash, accompanied by Halaster's booming laugh."
    short_description: "This gold-painted stone coin, engraved with Halaster's rune, can be used to reverse the effects of elder runes, but it disappears after one use."
    references {
      name: "Waterdeep - Dungeon of the Mad Mage"
      id: "WTC C4659"
      pages {
        low: 197
        high: 197
      }
    }
    images: "halaster's coin.png"
  }
  type: ADVENTURING_GEAR
  subtype: WONDROUS_ITEM
  rarity: RARE
  size: TINY
  substance {
    material: STONE
    thickness {
      inches {
        nominator: 1
        denominator: 4
      }
    }
  }
  player_name: "Coin"
}

items {
  common {
    name: "Pot of Awakening"
    worlds: "Generic"
    description: "A pot of awakening looks like a regular clay pot. If an ordinary shrub is planted in this pot and left to grow for at least 30 days, the shrub is transformed into an awakened shrub. The awakened shrub wakes and leaves the pot, destroying it in the process as its roots break through. The awakened shrub is friendly towards the individual who planted it."
    short_description: "If an ordinary shrub is planted in this pot and left to grow for at least 30 days, the shrub is transformed into an awakened shrub."
    references {
      name: "Waterdeep: Dungeon of the Mad Mage"
      id: "WTC C4659"
      pages {
        low: 42
        high: 42
      }
    }
    images: "pot of awakening.jpg"
  }
  type: MISC_MAGIC
  subtype: WONDROUS_ITEM
